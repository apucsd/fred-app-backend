model Review {
   id String @id @default(auto()) @map("_id") @db.ObjectId
   feedback String
   rating Int
   reviewedUserId  String @db.ObjectId @unique
   reviewAuthorId  String @db.ObjectId @unique
   createdAt DateTime @default(now())
   updatedAt DateTime @updatedAt
  reviewedUser User @relation("ReviewedUser", fields: [reviewedUserId], references: [id])
  reviewAuthor User @relation("ReviewAuthor", fields: [reviewAuthorId], references: [id])


  


   @@map("reviews")
}